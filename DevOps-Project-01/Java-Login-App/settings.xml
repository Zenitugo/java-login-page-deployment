<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.2.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.2.0 http://maven.apache.org/xsd/settings-1.2.0.xsd">
  
  <servers>
    <!-- You can leave this section empty or configure it for your own credentials if needed -->
  </servers>
  
  <profiles>
    <profile>
      <repositories>
        <!-- Maven Central repository for general dependencies -->
        <repository>
          <id>central</id>
          <url>https://repo.maven.apache.org/maven2</url>
        </repository>
      </repositories>
      <pluginRepositories>
        <!-- Maven Central repository for plugin dependencies -->
        <pluginRepository>
          <id>central</id>
          <url>https://repo.maven.apache.org/maven2</url>
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>

  <activeProfiles>
    <!-- Activate the 'central' profile by default -->
    <activeProfile>central</activeProfile>
  </activeProfiles>

</settings>

